<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mn.youmengna0.learnperssion">
    <permission android:name="com.mn.youmengna0.learnperssion.permission.MyATY_Permission" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyActivity" android:permission="com.mn.youmengna0.learnperssion.permission.MyATY_Permission">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="com.mn.youmengna0.learnperssion.intent.action.MyAty" />
            </intent-filter>
        </activity>
    </application>

</manifest>